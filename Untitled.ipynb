    "import numpy as np \n",
    "from sklearn.linear_model import LinearRegression\n",
    "\n",
    "X = np.array([[1] , [2] , [3] , [4] , [5] ])\n",
    "y = np.array([1 , 2 ,3 , 3.5 , 5])\n",
    "\n",
    "model = LinearRegression()\n",
    "model.fit(X,y)\n",
    "\n",
    "y_pred = model.predict(X)\n",
    "print(\"Prediction :\", y_pred)"
   ]
